{
    "name": "altman/respond",
    "type": "library",
    "description": "Easy JSON response for Laravel and Lumen, a useful package for handle your RESTful API endpoint",
    "keywords": [
        "altman",
        "Respond",
        "json"
    ],
    "homepage": "https://github.com/anetwork",
    "license": "MIT",
    "authors": [
        {
            "name": "Pemiphilo",
            "email": "sh.payro@anetwork.ir",
            "homepage": "http://anetwork.ir",
            "role": "Developer"
        }
    ],
    "require": {
        "illuminate/support": "~5.1",
        "php" : "~5.5|~7.0"
    },
    "require-dev": {
        "phpunit/phpunit" : "4.*",
        "scrutinizer/ocular": "~1.1",
        "squizlabs/php_codesniffer": "~2.3"
    },
    "autoload": {
        "psr-4": {
            "Altman\\Respond\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Altman\\Respond\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}

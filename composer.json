{
    "name": "vahid/respond",
    "type": "library",
    "description": "Easy JSON response for Laravel and Lumen, a useful package for handle your RESTful API endpoint",
    "keywords": [
        "vahid",
        "respond",
        "json"
    ],
    "homepage": "https://github.com/vahid-almasi",
    "license": "MIT",
    "require": {
        "illuminate/support": "~5.1",
        "php" : "^5.5"
    },
    "require-dev": {
        "phpunit/phpunit" : "4.*",
        "scrutinizer/ocular": "~1.1",
        "squizlabs/php_codesniffer": "~2.3"
    },
    "autoload": {
        "psr-4": {
            "Vahid\\Respond\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Vahid\\Respond\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}
